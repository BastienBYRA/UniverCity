            method="POST"
            onSubmit="if(!confirm('Voulez vous vraiment supprimer cet event ?')){return false;}">
             <button type="submit">
               Delete
             </button>
           </form>
      <p>Nom : {{this.lastName}}</p>
      <p>Pr√©nom : {{this.firstName}}</p>
      <p>email : {{this.email}}</p>
      <p>INE : {{this.INE}}</p>
      <p>login : {{this.login}}</p>
      <p>password : {{this.password}}</p>
      <p>Subjects : {{#each this.subjects}} <span>{{this.name}}</span> {{/each}}</p>
    </li>
  {{/each}}
  {{#each events}}
    <p>{{this.name}}</p>
  {{/each}}
</ul>