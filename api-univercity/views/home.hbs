<div id="add-button-research-bar-container">
  <h1>ALed</h1>
</div>
<button><a href="/api/subjects/add">Add subject</a></button>
<button><a href="/api/users/add">Add user</a></button>

<form action="/api/users/login" method="post">
  <label for="email"></label>
  <input type="text" placeholder="email" name="email" id="email">
  <label for="password"></label>
  <input type="text" placeholder="password" name="password" id="password">
  <input type="submit" value="Aled">
</form>
<ul>
  {{#each users}}
    <li>
      <button><a href="/api/users/edit/{{this._id}}">Editer</a></button>
      <form action="/api/users/delete/{{this._id}}"
            method="POST"
            onSubmit="if(!confirm('Voulez vous vraiment supprimer cet event ?')){return false;}">
             <button type="submit">
               Delete
             </button>
           </form>
      <p>Nom : {{this.lastName}}</p>
      <p>Pr√©nom : {{this.firstName}}</p>
      <p>email : {{this.email}}</p>
      <p>INE : {{this.INE}}</p>
      <p>login : {{this.login}}</p>
      <p>password : {{this.password}}</p>
      <p>Subjects : {{#each this.subjects}} <span>{{this.name}}</span> {{/each}}</p>
    </li>
  {{/each}}
  {{#each events}}
    <p>{{this.name}}</p>
  {{/each}}
</ul>